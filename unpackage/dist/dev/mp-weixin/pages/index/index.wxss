
.Location {
	width: 70%;
	margin: 0 auto;
	padding: 100rpx 0;
}
.img {
	width: 10%;
	height: 40rpx;
}
.text {
	margin-left: 20rpx;
}


.content {
	height: 100vh;
	width: 100vw;
	background-image: url(data:image/jpeg;base64,);
}
.button {
	width: 70%;
	margin: 0 auto;
	display: flex;
	flex-direction: row;
}
.abc {
	width: 23%;
	height: 95rpx;
	text-align: center;
	line-height: 100rpx;
	background: red;
	border-radius: 50%;
	margin: 0 20rpx;
}
.drink {
	width: 23%;
	height: 95rpx;
	text-align: center;
	line-height: 100rpx;
	background: #007AFF;
	border-radius: 50%;
	margin: 0 20rpx;
}
.content-data {
	width: 85%;
	height: 550rpx;
	border: 1px solid red;
	margin: 40rpx auto;
}
.vIf {
	line-height: 550rpx;
	text-align: center;
	color: red;
	height: 0rpx;
}
.list {
	width: 80%;
	height: 500rpx;
	margin: auto;
}
.select-list {
	overflow: auto;
}
.select-button {
	float: left;
	width: 25%;
	text-align: center;
	margin-top: 10rpx;
	color: red;
}
.item {
	height: 45rpx;
	line-height: 50rpx;
	margin: 2rpx 30rpx;
	border: 1rpx solid #d2000f;
	border-radius: 30rpx;
	font-size: 25rpx;
	text-align: center;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	color: #d2000f;
	background-color: #abd8cf;
	opacity: 0.8;
}
.screen {
	width: 20%;
	margin: 0 auto;
	-webkit-animation-name: example;
	        animation-name: example;
	-webkit-animation-duration: 4s;
	        animation-duration: 4s;
}
.but {
	width: 85%;
	margin: 0 auto;
	display: flex;
	flex-direction: row;
}
.sel-but-drink {
	color: #0000FF;
}
.but text {
	color: rgb(0, 170, 127);
	line-height: 74rpx;
}
@-webkit-keyframes example {
0% {
		background-color: red;
}
25% {
		background-color: yellow;
}
50% {
		background-color: blue;
}
100% {
		background-color: green;
}
}
@keyframes example {
0% {
		background-color: red;
}
25% {
		background-color: yellow;
}
50% {
		background-color: blue;
}
100% {
		background-color: green;
}
}
.display {
	text-align: center;
	margin-top: 100rpx;
	color: #007AFF;
}
.none {
	display: none;
}

